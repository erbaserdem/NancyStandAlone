<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>NancyAspNetHost1</title>

	<style type="text/css">

		body {
			text-align: center;
		}

	</style>

</head>
<body>
	<img src="@Path['~/Content/nancy-logo.png']" alt="Nancy logo" /><br />
	Total of @Model.Count users are found  <br>
	Found Users are :  <br>
	@Each
		@Current.Name	<br>
		@Current.Email	<br>
		@Current.UserName	<br>
		@Current.Id <br>
	@EndEach


	<form action="~/" method="get">
    <input type="submit" value="MainMenu"/>
	</form>


</body>
</html>
